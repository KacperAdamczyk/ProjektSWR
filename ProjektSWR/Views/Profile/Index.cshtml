@model ProjektSWR.Models.ProfileModel
@{
    ViewBag.Title = "Profile";
}

@Styles.Render("~/Content/UPPS/one.css")

<div id="mainbeam">
    <h3>Dane personalne</h3>
</div>
<div id="space" style="padding-bottom: 20px;">
    <div id="photo">
        <img src="https://branzadziecieca.pl/wp-content/uploads/2016/10/anek-WhaWhaWhacky-Kaktus-web.jpg" alt=" "
             height="142" width="97" />
    </div>
    <center>
        <div id="swimmingpool">
            <h4>@Html.DisplayFor(m => m.AcademicDegree) @Html.DisplayFor(m => m.FirstName) @Html.DisplayFor(m => m.LastName)</h4>
        </div>
    </center>
    <div id="swimmingpool">
        Katedra: @Html.DisplayFor(m => m.CathedralName)
    </div>
    <div id="swimmingpool">
        Telefon: @Html.DisplayFor(m => m.PhoneNumber)
    </div>
    <div id="swimmingpool">
        E-mail: @Html.DisplayFor(m => m.Email)
    </div>
    <div id="swimmingpool">
        Data urodzenia: @Html.DisplayFor(m => m.DateOfBirth)
    </div>
    <div id="swimmingpool">
        Opis: @Html.DisplayFor(m => m.Description)
    </div>

</div>

<div id="mainbeam">
    <h3>Kalendarz</h3>

</div>
